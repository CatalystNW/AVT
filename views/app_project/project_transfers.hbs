<div id="site_assessment_container">
    <h2>Completed Site Assessments</h2>
    <ul>
        {{#each assessments}}
        <li>
            <a href="./project_transfer/{{this.id}}">
                {{this.documentPackage.application.name.first}} {{this.documentPackage.application.name.last}}
            </a>
        </li>
        {{/each}}
    </ul>
</div>

<script src="https://unpkg.com/react@17/umd/react.development.js" crossorigin></script>
<script src="https://unpkg.com/react-dom@17/umd/react-dom.development.js" crossorigin></script>

<!-- Use JSX Preprocessor for production.
    This is used as a quick way to get JSX formatting, but it's very slow for browsers 
